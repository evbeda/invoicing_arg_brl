[run]
source =
    invoicing_app

omit =
    invoicing_app/migrations/*
    invoicing_app/apps.py
    invoicing_app/views.py
    invoicing_app/admin.py
    invoicing_app/models.py
    invoicing/*
    manage.py

[report]
exclude_lines =
    pragma: no cover
    def setUp
    def test_
    return raw_input
    print
    if __name__ == '__main__':
    def suite
